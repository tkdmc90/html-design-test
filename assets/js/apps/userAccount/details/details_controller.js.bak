define(["app", "apps/userAccount/details/details_view"], function (BrewOptix, View) {
    BrewOptix.module("UserAccountApp.Details", function (Details, BrewOptix, Backbone, Marionette, $, _) {
        Details.Controller = {
            showDetails: function(id) {
                require(["entities/userAccount"], function() {

                    var userAccount = BrewOptix.request("userAccount:entity", id);

                    var view = new View.UserAccount({ model: userAccount });

                    BrewOptix.mainRegion.show(view);
                });
            }
        };

    });

    return BrewOptix.UserAccountApp.Details.Controller;
});
